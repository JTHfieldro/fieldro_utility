{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "catkin_make: build",
      "type": "shell",
      "command": "catkin_make",
      "args": [
        "-DCMAKE_BUILD_TYPE=Debug"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}